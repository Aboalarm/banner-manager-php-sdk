{
    "name": "aboalarm/banner-manager-sdk",
    "license": "",
    "description": "Banner Manager API integration for laravel",
    "keywords": ["banner", "aboalarm", "laravel"],
    "authors": [
        {
            "name": "Evis Bregu",
            "email": "evis.ext@aboalarm.de"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Aboalarm\\BannerManagerSdk\\": "src/",
            "Aboalarm\\BannerManagerSdk\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aboalarm\\BannerManagerSdk\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Banner": "Aboalarm\\BannerManagerSdk\\Laravel\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
